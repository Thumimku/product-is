        "properties": [
          {
            "key": "min.length",
            "value": "1"
          }
        ]
      },
      {
        "validator": "LowerCaseValidator",
        "properties": [
          {
            "key": "min.length",
            "value": "1"
          }
        ]
      },
      {
        "validator": "SpecialCharacterValidator",
        "properties": [
          {
            "key": "min.length",
            "value": "1"
          }
        ]
      }
    ]
  },
  {
    "field": "username",
    "rules": [
      {
